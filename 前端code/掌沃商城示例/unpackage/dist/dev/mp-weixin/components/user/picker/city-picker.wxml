<block wx:if="{{!hidden}}"><view class="city-picker"><view class="card-header"><block wx:if="{{province.name}}"><text data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="address-item" bindtap="__e">{{province.name}}</text></block><block wx:if="{{city.name}}"><text data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="address-item" bindtap="__e">{{city.name}}</text></block><block wx:if="{{district.name}}"><text class="address-item">{{district.name}}</text></block><text class="title">{{pickerTitle}}</text><view data-event-opts="{{[['tap',[['$emit',['update:hidden',true]]]]]}}" class="icon i-close _i" bindtap="__e"></view></view><view class="card-body"><block wx:for="{{cities}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['change',[index,'$0'],[[['cities','',index,'name']]]]]]]}}" class="item" bindtap="__e">{{item.name}}</view></block></view></view></block>